int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
    if (n % 10 > 5)
    {
        printf("%d is greater than 5 ");
    }
  if (n % 10 == 5)
    {
        printf("%d and is 0 ");
    }
    if (n % 10 < 6)
    {
        printf("%d is less than 6 and not 0 ");
    }
